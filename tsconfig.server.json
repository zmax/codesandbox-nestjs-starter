{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": ".next/production-server",
    "baseUrl": "./",
    "incremental": true,
    "moduleResolution": "node",
    "noEmit": false
  },
  "include": ["./src/**/*"],
  "exclude": [
    "node_modules",
  ]
}